<!-- Left Sidenav -->
<div class="left-sidenav">
	<!-- begin logo -->
	<div class="brand">
		<a href="/admin/generate-tools" class="logo">
			<span>
				<img src="../../../template/admin/dastone/default/assets/images/logo-sm.png" alt="logo-small" class="logo-sm">
			</span>
			<span>
				<img src="../../../template/admin/dastone/default/assets/images/logo.png" alt="logo-large" class="logo-lg logo-light">
				<img src="../../../template/admin/dastone/default/assets/images/logo-dark.png" alt="logo-large" class="logo-lg logo-dark">
			</span>
		</a>
	</div>
	<!--end logo-->
	
	<%
		let arrUrl = [
    
			// MARK
		];

		let arrUrlGroup = [];
	%>

	<div class="menu-content h-100" data-simplebar>
		<ul class="metismenu left-sidenav-menu">

			<% if (ROLE_USER === 'SUPERVISOR') { %>
                <li class="menu-label mt-0">SUPERVISOR</li>
				<li>
					<a href="javascript: void(0);"> <i data-feather="home" class="align-self-center menu-icon"></i><span>Dashboard</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
					<ul class="nav-second-level" aria-expanded="false">
						<li class="nav-item">
							<a class="nav-link" href="/admin/generate-tools">
								<i class="ti-control-record"></i>
								Generator Package
							</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="/admin/trigger-event">
								<i class="ti-control-record"></i>
								Trigger Event
							</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="/admin/generate-charts">
								<i class="ti-control-record"></i>
								Generator Chart
							</a>
						</li>
					</ul>
				</li>
                <li>
                    <a href="javascript: void(0);"> <i class="fas fa-lock"></i><span>User Management</span><span class="menu-arrow"><i class="mdi mdi-chevron-right"></i></span></a>
                    <ul class="nav-second-level" aria-expanded="false">
                        <li class="nav-item">
                            <a class="nav-link" href="/admin/roles">
                                <i class="ti-control-record"></i>
                                Roles
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/admin/apis">
                                <i class="ti-control-record"></i>
                                APIs
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/admin/users">
                                <i class="ti-control-record"></i>
                                Users
                            </a>
                        </li>
                    </ul>
                </li>
            <% } %>

			<hr class="hr-dashed hr-menu">
            <li class="menu-label my-2">Quản Lý Admin</li>

            <li>
                <a href="javascript: void(0);">
                    <i class="fas fa-user-shield"></i>
                    <span>Quản lý admin</span>
                    <span class="menu-arrow">
                        <i class="mdi mdi-chevron-right"></i>
                    </span>
                </a>
                <ul class="nav-second-level" aria-expanded="false">
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/list-admin">
                            <i class="ti-control-record"></i>
                            Danh sách admin
                        </a>

                    </li>
                </ul>
            </li>

			<% arrUrl.forEach((urlObj, index) => { %>
				<% if(scopes.includes(urlObj.scope) || ROLE_USER === 'SUPERVISOR') { %>

					<% if(!arrUrlGroup.includes(urlObj.group)){ %>
						<hr class="hr-dashed hr-menu">
				        <li class="menu-label my-2"> <%= urlObj.header %> </li>
						<% arrUrlGroup = [...arrUrlGroup, urlObj.group]; %>
					<% } %>

					<li>
			            <a href="javascript: void(0);">
			                <%- urlObj.icon %>
			                <span> <%= urlObj.name %> </span>
			                <span class="menu-arrow">
			                    <i class="mdi mdi-chevron-right"></i>
			                </span>
			            </a>
			            <ul class="nav-second-level" aria-expanded="false">
			                <li class="nav-item">
								<% urlObj.childs.forEach(urlObjChild => { %>

									<% if(scopes.includes(urlObjChild.scope) || ROLE_USER === 'SUPERVISOR'){ %>
										<a class="nav-link" href="<%= urlObjChild.path %>">
											<i class="ti-control-record"></i>
											<%= urlObjChild.name %>
										</a>
									<% } %>

					            <% }) %>
			                </li>
			            </ul>
			        </li>
				<% } %>

	        <% }) %>

		</ul>

		<!-- <div class="update-msg text-center">
			<a href="javascript: void(0);" class="float-end close-btn text-muted" data-dismiss="update-msg" aria-label="Close" aria-hidden="true">
				<i class="mdi mdi-close"></i>
			</a>
			<h5 class="mt-3">Mannat Themes</h5>
			<p class="mb-3">We Design and Develop Clean and High Quality Web Applications</p>
			<a href="javascript: void(0);" class="btn btn-outline-warning btn-sm">Upgrade your plan</a>
		</div> -->
	</div>
</div>
<!-- end left-sidenav-->
